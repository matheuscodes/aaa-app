{
  "name": "aaa-app",
  "version": "1.3.1",
  "description": "AAA - Advanced Archery Application",
  "main": "src/server.js",
  "scripts": {
    "bundle": "browserify src/app.js -t babelify --outfile build/app.js --extension=.jsx --extension=.svg",
    "build": "rm -rf build && mkdir build && npm run compile && npm run uglify",
    "build-production": "env-cmd production npm run build",
    "clean": "rm -rf node_modules && rm -rf build",
    "compile": "babel src -d build",
    "eslint": "eslint --ext .jsx,.svg --fix ",
    "fetch-languages": "rm -rf node_modules/aaa-languages && cp -R ../aaa-languages node_modules/",
    "revert-languages": "rm -rf node_modules/aaa-languages && npm run install aaa-languages",
    "serve": "npm run bundle && babel-node src/server.js",
    "serve-development": "env-cmd development npm run serve",
    "serve-production": "env-cmd production node build/server.js",
    "uglify": "npm run bundle && terser build/app.js -o build/app.js",
    "deploy-domain": "serverless create_domain",
    "delete-domain": "serverless delete_domain",
    "delete-stack": "serverless remove",
    "just-deploy": "serverless deploy",
    "deploy-stack": "npm run build-production && serverless deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheuscodes/aaa-app.git"
  },
  "keywords": [
    "archery",
    "performance"
  ],
  "author": "Matheus Borges Teixeira",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/matheuscodes/aaa-app/issues"
  },
  "homepage": "https://github.com/matheuscodes/aaa-app#readme",
  "dependencies": {
    "aaa-languages": "git+https://github.com/matheuscodes/aaa-languages.git",
    "aws-serverless-express": "3.3.6",
    "core-decorators": "0.19.0",
    "express": "4.17.1",
    "i18next": "8.4.3",
    "i18next-browser-languagedetector": "2.0.0",
    "i18next-express-middleware": "1.0.5",
    "i18next-node-fs-backend": "2.1.3",
    "i18next-xhr-backend": "1.4.2",
    "keycode": "2.1.9",
    "material-ui": "0.18.6",
    "mobile-detect": "1.3.6",
    "moment": "2.24.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-event-listener": "^0.4.5",
    "react-i18next": "4.6.2",
    "react-tap-event-plugin": "2.0.1",
    "serverless-domain-manager": "^2.6.10"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-inline-environment-variables": "0.1.1",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "14.4.0",
    "env-cmd": "5.1.0",
    "eslint": "4.19.1",
    "eslint-config-google": "0.9.1",
    "eslint-plugin-react": "7.1.0",
    "intl": "1.2.5",
    "prop-types": "15.5.10",
    "serverless": "1.64.0",
    "terser": "4.6.3"
  }
}
