{
  "name": "aaa-app",
  "version": "1.0.0",
  "description": "AAA - Advanced Archery Application",
  "main": "src/server.js",
  "scripts": {
    "uglify": "npm run bundle && uglifyjs build/app.js -o build/app.js",
    "bundle": "browserify src/app.js -t babelify --outfile build/app.js --extension=.jsx --extension=.svg",
    "compile": "babel src -d build",
    "build": "rm -rf build && mkdir build && npm run compile && npm run uglify",
    "build-production":"env-cmd production npm run build",
    "serve": "npm run bundle && babel-node src/server.js",
    "serve-development": "env-cmd development npm run serve",
    "serve-production": "env-cmd production node build/server.js",
    "eslint": "eslint --fix src --ext .jsx,.svg",
    "clean": "rm -rf node_modules && rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheuscodes/aaa-app.git"
  },
  "keywords": [
    "archery",
    "performance"
  ],
  "author": "Matheus Borges Teixeira",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/matheuscodes/aaa-app/issues"
  },
  "homepage": "https://github.com/matheuscodes/aaa-app#readme",
  "dependencies": {
    "aaa-languages": "git+https://github.com/matheuscodes/aaa-languages.git",
    "app-module-path": "^2.2.0",
    "express": "^4.14.0",
    "i18next": "^5.0.0",
    "i18next-browser-languagedetector": "^1.0.1",
    "i18next-express-middleware": "^1.0.2",
    "i18next-node-fs-backend": "^0.1.3",
    "i18next-xhr-backend": "^1.3.0",
    "material-ui": "^0.16.7",
    "moment": "^2.17.1",
    "node-jsx": "^0.13.3",
    "node-sass": "^4.3.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-i18next": "^2.0.0",
    "react-tap-event-plugin": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-inline-environment-variables": "0.0.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "env-cmd": "^4.0.0",
    "eslint": "^3.13.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-react": "^6.9.0",
    "uglify-js": "^2.7.5"
  }
}
